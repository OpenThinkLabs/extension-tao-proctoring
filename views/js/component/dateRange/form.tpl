<form class="daterange datepicker">
    <label>{{__ "From"}}</label><input type="text" name="periodStart" value="{{start}}"/>
    <label>{{__ "to"}}</label><input type="text" name="periodEnd" value="{{end}}"/>
    <button class="small info" data-control="filter"><span class="icon icon-reset"></span>{{__ "Refresh"}}</button>
</form>